<!DOCTYPE html>
<html>
<head>
  <title>User List</title>
  <style>
    table { border-collapse: collapse; width: 80%; margin: 20px auto; }
    th, td { border: 1px solid #ccc; padding: 10px; text-align: left; }
    th { background-color: #f4f4f4; }
  </style>
</head>
<body>
  <h2 style="text-align:center;">User List</h2>
  <table>
    <thead>
      <tr>
        <th>Name</th><th>Username</th><th>Phone</th><th>Email</th><th>Role</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.username %></td>
          <td><%= user.phone %></td>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
